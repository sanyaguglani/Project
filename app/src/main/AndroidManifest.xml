<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.sportit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AttendingEvents"
            android:label="Attending Events"
            />

        <activity android:name=".ViewAllEvents"
            android:label="View Events"
            />
        <activity android:name=".MyEvents"
            android:label="My Events"
            />

        <activity android:name=".EventDetails"
            android:label="Events"
            />
    </application>

</manifest>